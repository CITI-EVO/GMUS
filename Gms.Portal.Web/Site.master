        <script type="text/javascript">
            $(document).ready(function () {
                $('.colorpicker').colorpicker();

                $('.tableStd').DataTable({
                    //dom: '<"left"f>rt<"bottom"lpb>'
                    dom: 'ifrtlpb',
                    stateSave: true,
                });

                $('.tableIntern').DataTable({
                    dom: 'rtp',
                    stateSave: true
                });

                $('.clockpicker').clockpicker();

                $('.chosen-select').chosen({
                    width: "100%"
                });

                //$(".chosen-select").select2({
                //    placeholder: "Select a state",
                //    allowClear: true
                //});

                $(".intSpinEdit").TouchSpin({
                    max: 2147483647,
                    buttondown_class: 'btn btn-white',
                    buttonup_class: 'btn btn-white'
                });

                $(".decSpinEdit").TouchSpin({
                    step: 0.1,
                    decimals: 2,
                    max: 2147483647,
                    buttondown_class: 'btn btn-white',
                    buttonup_class: 'btn btn-white'
                });

                $(".elementSizeSpinEdit").TouchSpin({
                    min: 1,
                    max: 12,
                    buttondown_class: 'btn btn-white',
                    buttonup_class: 'btn btn-white'
                });

                $(".groupSizeSpin").TouchSpin({
                    min: 1,
                    max: 12,
                    buttondown_class: 'btn btn-white',
                    buttonup_class: 'btn btn-white'
                });

                $.fn.datepicker.defaults.format = "dd.mm.yyyy";

                $('.input-group.date').datepicker({
                    todayBtn: "linked",
                    keyboardNavigation: false,
                    forceParse: false,
                    calendarWeeks: true,
                    autoclose: true
                });

                $('.input-daterange').datepicker({
                    keyboardNavigation: false,
                    forceParse: false,
                    autoclose: true
                });

                $('.summernote').summernote();

                $('.i-checks').iCheck({
                    checkboxClass: 'icheckbox_square-green',
                    radioClass: 'iradio_square-green',
                });

            });

            $(function () {
                $(".move-top").click(function () {
                    $("html, body").animate({ scrollTop: 0 }, "slow");
                    return false;
                });

                // for bootstrap 3 use 'shown.bs.tab', for bootstrap 2 use 'shown' in the next line
                $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                    // save the latest tab; use cookies if you like 'em better:
                    localStorage.setItem('lastTab', $(this).attr('href'));
                });

                // go to the latest tab, if it exists:
                var lastTab = localStorage.getItem('lastTab');
                if (lastTab) {
                    $('[href="' + lastTab + '"]').tab('show');
                }
            });

            function onSave() {
                var val = Page_ClientValidate();

                if (!val) {
                    for (var i = 0; i < Page_Validators.length; i++) {
                        var validator = Page_Validators[i];
                        var elemnId = validator.controltovalidate;

                        $("#" + elemnId).css("background-color", "white");

                        if (!validator.isvalid) {
                            $("#" + elemnId).css("background-color", "rgb(255, 184, 184)");
                        }
                    }
                }

                return val;
            }
        </script>

