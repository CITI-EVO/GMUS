<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Gms.Portal.DAL"
                   namespace="Gms.Portal.DAL.Domain">

  <class name="GM_Form" table="GM_Forms">
    <id name="ID">
      <generator class="assigned" />
    </id>
    <property name="Name" />
    <property name="Number" />
    <property name="OrderIndex" />
    <property name="Visible" />
    <property name="RequiresApprove" />
    <property name="ApprovalDeadline" />
    <property name="VisibleExpression" />
    <property name="FillingValidationExpression" />
    <property name="FillingValidationMessage" />
    <property name="UserMode" />
    <property name="XmlData" type="System.Xml.Linq.XDocument">
      <column name="XmlData" sql-type="xml" />
    </property>
    <property name="CategoryID" />
    <property name="DateCreated" />
    <property name="DateChanged" />
    <property name="DateDeleted" />
  </class>
</hibernate-mapping>